<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.facebookweb.entity.FacebookEmployee" table="CONTACT">
	   <id name="f_id" type="long" column="ID" >
	  	
	  </id>
	  

	  <property name="name">
		 <column name="FIRSTNAME" />
	  </property>
	  <property name="pass">
		<column name="password"/>
	  </property>
	
  <property name="email">
		<column name="EMAIL"/>
	  </property>
	  
	    <property name="address">
		<column name="address"/>
	  </property>
   </class>

  

</hibernate-mapping>